{
  "gitbook": ">= 3.2.2",
  "title": "DrjLab User Guide",
  "description": "",
  "language": "zh-hans",
  "plugins": [
    "youtube",
    "richquotes",
    "collapsible-menu",
    "back-to-top-button",
    "page-toc-button",
    "mermaid",
    "language-picker",
    "custom-favicon",    
    "toolbar@git+https://github.com/hamishwillee/gitbook-plugin-toolbar.git",
    "mathjax@git+https://github.com/Dronecode/plugin-mathjax.git#update_cdn",
    "versions-select@git+https://github.com/Dronecode/gitbook-plugin-versions-select.git"
  ],
  "pluginsConfig": {
    "richquotes": {
        "tip": {
            "alert": "success",
            "picto": "fa-thumbs-o-up"
        }

    },
    "toolbar": {
      "buttons": [
        {
          "label": "Bug tracker",
          "icon": "fa fa-bug",
          "position": "left",
          "url": "https://github.com/drjlab/drjlab-user-guide/issues/new?title=Doc+Bug:+{{title}}&body=DESCRIBE+PROBLEM+WITH+DOCS+HERE%0A%0ABug+Page:+[{{title}}]({{url}})"
        },
        {
          "label": "Edit page on github",
          "icon": "fa fa-pencil-square-o",
          "position": "left",
          "url": "https://github.com/drjlab/drjlab-user-guide"
        },
        {
          "label": "GitHub",
          "icon": "fa fa-github",
          "url": "https://github.com/drjlab/drjlab-user-guide/edit/master/{{filepath_lang}}"
        }
      ]
    }
  }
}